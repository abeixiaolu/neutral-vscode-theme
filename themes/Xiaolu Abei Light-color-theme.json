  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#a3a3a3"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.member",
        "variable.other.enummember",
        "variable.language",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#565690"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#567890"
      }
    },
    {
      "scope": [
        "variable.parameter.function",
        "meta.object.member"
      ],
      "settings": {
        "foreground": "#565690"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#569056"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#569056",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": [
        "keyword.operator.word"
      ],
      "settings": {
        "foreground": "#569056"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#569056",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#569056"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#565690"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#907356"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#565690"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#565690"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#905656"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#905656"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#905656"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#905656"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#569056"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#905656"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#567890"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#567890"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#569056"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#565690"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#333"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#569056"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#565690"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#565690"
      }
    },
    {
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#905656"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#569056"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#565690"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "background": "#333"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#567890",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#333"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#a3a3a3"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#905656"
      }
    },
    {
      "scope": [
        "constant.other.reference.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#567890",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace.round"
      ],
      "settings": {
        "foreground": "#a3a3a3"
      }
    },
    {
      "scope": [
        "text.html"
      ],
      "settings": {
        "foreground": "#333"
      }
    }
  ]
}