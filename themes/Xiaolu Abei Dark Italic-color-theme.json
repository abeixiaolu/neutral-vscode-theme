        "foreground": "#d1cdc2"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#d1cdc2"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#d1cdc2"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#cc8787"
      }
    },
    {
      "scope": [
        "constant.other.reference.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#94b5c0",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace.round"
      ],
      "settings": {
        "foreground": "#d1cdc2"
      }
    },
    {
      "scope": [
        "text.html"
      ],
      "settings": {
        "foreground": "#d1cdc2"
      }
    }
