  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#737373"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.member",
        "variable.other.enummember",
        "variable.language",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#8ebf9d"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#94b5c0"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#6eb4b4"
      }
    },
    {
      "scope": [
        "variable.parameter.function",
        "meta.object.member"
      ],
      "settings": {
        "foreground": "#8ebf9d"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#9d94bf"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#9d94bf",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#d1cdc2",
        "fontStyle": "normal"
      }
    },
    {
      "scope": [
        "keyword.operator.word"
      ],
      "settings": {
        "foreground": "#9d94bf"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#9d94bf",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#8ebf9d"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#8ebf9d"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#cc8787"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#d1cdc2"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#8ebf9d"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#8ebf9d"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cc8787"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cc8787"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cc8787"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cc8787"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#9d94bf"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#cc8787"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#d1cdc2"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#94b5c0"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#94b5c0"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#9d94bf"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#d1cdc2"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#d1cdc2"
      }
