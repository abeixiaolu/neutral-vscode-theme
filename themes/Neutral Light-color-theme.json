    {
      "scope": [
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#b69c18"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#27282460"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.member",
        "variable.other.enummember",
        "variable.language",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#ab6b18"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
